config_version=5

[application]

config/name="Find Billy!"
config/description="A 2 dimensional Pixel Art Jump & Run. Your mission is it to find Billy!"
run/main_scene="res://levels/main.tscn"
config/features=PackedStringArray("4.3")
boot_splash/bg_color=Color(0.196078, 0.368627, 0.733333, 1)
boot_splash/image="res://brand/banner.png"
boot_splash/use_filter=false
config/icon="res://build/icons/usr/share/icons/hicolor/128x128/apps/find-billy.png"
config/macos_native_icon="res://build/icons/usr/share/icons/hicolor/128x128/apps/find-billy.icns"
config/windows_native_icon="res://build/icons/usr/share/icons/hicolor/128x128/apps/find-billy.ico"

[audio]

driver="Pipewire"

[autoload]

global="*res://global.gd"

[display]

window/size/viewport_width=320
window/size/viewport_height=180
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
mouse_cursor/custom_image="res://ui/cursor.png"
window/size/fullscreen=true

[gui]

theme/custom_font="res://fonts/montreal/Montreal-Regular.ttf"

